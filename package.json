{
  "name": "avol-invoice",
  "version": "1.0.0",
  "description": "Hybrid Desktop application to manage inventory",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && concurrently \"npm run tsc:w\" \"electron main.js\"",
    "clean": "rm -Rf *.log *.js *.map app/*.js app/*.map",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install",
    "tsc": "tsc",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "engines": {
    "node": "5.9.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gandhichintan/avol-invoice.git"
  },
  "keywords": [
    "electron",
    "node",
    "angular2",
    "desktop",
    "hybrid"
  ],
  "author": "Chintan Gandhi",
  "bugs": {
    "url": "https://github.com/gandhichintan/avol-invoice/issues"
  },
  "homepage": "https://github.com/gandhichintan/avol-invoice#readme",
  "dependencies": {
    "@angular/common": "^2.4.10",
    "@angular/compiler": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/forms": "~2.4.9",
    "@angular/http": "^2.4.10",
    "@angular/material": "^2.0.0-alpha.11-3",
    "@angular/platform-browser": "^2.4.10",
    "@angular/platform-browser-dynamic": "^2.4.10",
    "@angular/router": "~3.4.9",
    "@angular/upgrade": "~2.4.9",
    "@types/node": "^8.0.13",
    "electron": "^1.6.11",
    "jquery": "~3.1.1",
    "ng-chartist": "1.1.0",
    "ngx-electron": "^1.0.1",
    "reflect-metadata": "~0.1.8",
    "rxjs": "5.0.1",
    "sqlite3": "^3.1.8",
    "sqlite3-offline": "^3.1.8-2",
    "systemjs": "^0.19.47",
    "zone.js": "^0.7.8"
  },
  "devDependencies": {
    "@types/chartist": "~0.9.33",
    "@types/core-js": "0.9.35",
    "@types/jquery": "^2.0.48",
    "@types/node": "~6.0.45",
    "concurrently": "~3.0.0",
    "electron-builder": "^19.6.3",
    "electron-packager": "^8.7.2",
    "electron-rebuild": "^1.5.11",
    "lite-server": "~2.2.2",
    "typescript": "~2.0.3",
    "typings": "^2.1.1"
  }
